			</Layer>
		</Layers>
		<NormalTexture parentKey="NormalTexture" atlas="UI-HUD-ActionBar-IconFrame">
			<Size x="45" y="45"/>
			<Anchors>
				<Anchor point="CENTER" x="1.5" y="-1"/>
